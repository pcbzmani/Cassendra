CREATE TABLE h1b_table(
CASE_NUMBER 	TEXT  	 PRIMARY KEY,
CASE_STATUS 	TEXT,	
CASE_SUBMITTED 	DATE,	
DECISION_DATE 	DATE,	
VISA_CLASS	 TEXT,	
EMPLOYMENT_START_DATE 	DATE,	
EMPLOYMENT_END_DATE DATE,	
EMPLOYER_NAME	 TEXT,	
EMPLOYER_BUSINESS_DBA 	TEXT,	
EMPLOYER_ADDRESS	 TEXT,	
EMPLOYER_CITY	 TEXT,	
EMPLOYER_STATE	 TEXT,	
EMPLOYER_POSTAL_CODE  	TEXT,	
EMPLOYER_COUNTRY	 TEXT,	
EMPLOYER_PROVINCE	 TEXT,	
EMPLOYER_PHONE	 BIGINT,	
EMPLOYER_PHONE_EXT 	TEXT,	
AGENT_REPRESENTING_EMPLOYER 	TEXT,	
AGENT_ATTORNEY_NAME  	VARCHAR,	
AGENT_ATTORNEY_CITY 	TEXT,	
AGENT_ATTORNEY_STATE 	TEXT,	
JOB_TITLE 	TEXT,	
SOC_CODE 	VARCHAR,	
SOC_NAME 	TEXT,	
NAICS_CODE 	INT,	
TOTAL_WORKERS 	INT,	
NEW_EMPLOYMENT	 INT,	
CONTINUED_EMPLOYMENT 	INT,	
CHANGE_PREVIOUS_EMPLOYMENT 	INT,	
NEW_CONCURRENT_EMPLOYMENT 	INT,	
CHANGE_EMPLOYER	 INT,	
AMENDED_PETITION	 INT,	
FULL_TIME_POSITION 	TEXT,	
PREVAILING_WAGE	 VARCHAR,	
PW_UNIT_OF_PAY	 TEXT,	
PW_WAGE_LEVEL	 VARCHAR,	
PW_SOURCE	 TEXT,	
PW_SOURCE_YEAR	 INT,	
PW_SOURCE_OTHER	 TEXT,	
WAGE_RATE_OF_PAY_FROM	 VARCHAR,	
WAGE_RATE_OF_PAY_TO	 VARCHAR,	
WAGE_UNIT_OF_PAY 	TEXT,	
H1B_DEPENDENT 	VARCHAR,	
WILLFUL_VIOLATOR 	VARCHAR,	
SUPPORT_H1B	 VARCHAR,	
LABOR_CON_AGREE	 VARCHAR,	
PUBLIC_DISCLOSURE_LOCATION	 VARCHAR,	
WORKSITE_CITY	 TEXT,	
WORKSITE_COUNTY	 TEXT,	
WORKSITE_STATE	 VARCHAR,	
WORKSITE_POSTAL_CODE 	VARCHAR,	
ORIGINAL_CERT_DATE	 DATE,	
);



alter table h1b_table add (email text);
create index employername on h1b_table(employer_name);


   
